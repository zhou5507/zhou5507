#include <iostream>
#include <string>
using namespace std;
int main(){
    int n{0};
    cin>>n;
    cin.ignore();
    for (int i{0};i<n;i++){
        string a,b;
        getline(cin,a,':');
        getline(cin,b);
        int LA{0},LB{0};
        LA=a.length();
        LB=b.length();
        int t1{0},t2{0};
        if(LA>=LB){
            cout <<"NO"<<endl;
            continue;
        }
        for(int i{0};i<LA;i++){
            for (int j{t1};j<LB;j++){
                if(a[i]==b[j]){
                    t1=j+1;
                    t2++;
                    break;
                }
            }
        }
        if (t2==LA){
            cout<<"YES"<<endl;
        }
        else{cout<<"NO"<<endl;}
    }
}
