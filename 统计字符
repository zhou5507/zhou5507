#include <iostream>
#include <vector>
#include <string>
using namespace std;
vector<string> WX()
{
    vector<string> words;
    string now = "";
    char
        alpha{};
    while (cin.get(alpha))
    {
        if ((alpha >= 'a' && alpha <= 'z') ||
            (alpha >='A' && alpha <= 'Z') ||
            (alpha >= '0' && alpha <= '9')||alpha==39)
        {
            now += alpha;
        }
        else
        {
            if (!now.empty())
            {
                words.push_back(now);
                now ="";
            }
        }
    }
    if (!now.empty())
    {
        words.push_back(now);
    }
    return words;
}
int main()
{
    int X[1000]{};
    vector<string> A, B;
    A = WX();
    int L{0};
    L = A.size();
    for (int i{0}; i < L; i++)
    {
        int G{0};
        G = B.size();
        int y{0};
        for (int j{0}; j < G; j++)
        {
            if (A[i] == B[j])
            {
                X[j]++;
                y++;
            }
        }
        if (y == 0)
        {
            B.push_back(A[i]);
            X[B.size() - 1] = 1;
        }
    }
    int F{0};
    F = B.size();
    for (int i{0}; i < F; i++)
    {
        cout << B[i] << " " << X[i] << endl;
    }
}
