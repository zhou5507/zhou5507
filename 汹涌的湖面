#include <iostream>
using namespace std;
int main() {
	int m{ 0 }, n{ 0 };
	int A[50][50]{};
	cin >> n >> m;
	int t{ 0 };
	for (int k{ 0 };k < n;k++) {
		for (int s{ 0 };s < m;s++) {
			int a{ 0 };
			cin >> a;
			A[k][s] = a;
		}
	}
	for (int s{ 0 };s < m;s++) {
		for (int k{ 0 };k < n-1;k++) {
			for (int i{ 0 };i < n - 1 - k;i++) {
				if (A[i][s] > A[i + 1][s]) {
					int y{ 0 };
					y = A[i][s];
					A[i][s] = A[i + 1][s];
					A[i + 1][s] = y;
				}
			}
		}
	}
	for (int k{ 0 };k < n;k++) {
		bool B{ true };
		for (int s{ 0 };s < m;s++) {
			if (B) {
				cout << A[k][s];
				B = false;
			}
			else {
				cout << " " << A[k][s];
			}

		}
		cout << endl;
	}
}
