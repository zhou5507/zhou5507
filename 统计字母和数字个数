#include <iostream>
#include <string>
using namespace std;
int main (){
    string A;
    getline(cin, A);
    int a{0},b{0},c{0};
    for (int i{0};i<A.length();i++){
        int t{A[i]};
        if(t<='Z'&&t>='A')a++;
        else if(t<='z'&&t>='a')b++;
        else if(t<='9'&&t>='0')c++;
        else continue;
    }
    cout<<a<<" "<<b<<" "<<c<<endl;
}
