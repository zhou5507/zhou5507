#include <iostream>
using namespace std;
int main()
{
    for (int i{1}; i <= 3; i++)
    {
        for (int j{1}; j <= 9; j++)
        {
            for (int l{1}; l <= 9; l++)
            {
                for (int a{2}; a <= 8; a++)
                {
                    for (int b{1}; b <= 9; b++)
                    {
                        for (int c{2}; c <= 9; c += 2)
                        {
                            for (int m{3}; m <= 9; m++)
                            {
                                for (int n{1}; n <= 9; n++)
                                {
                                    for (int q{1}; q <= 9; q += 1)
                                    {
                                        int w{0}, x{0}, p{0};
                                        w = 100 * i + 10 * j + l;
                                        x = 100 * a + 10 * b + c;
                                        p = 100 * m + 10 * n + q;
                                        bool W{
                                            i == j || i == l || i == a || i == b || i == c || i == m || i == n || i == q ||
                                            j == l || j == a || j == b || j == c || j == m || j == n || j == q ||
                                            l == a || l == b || l == c || l == m || l == n || l == q ||
                                            a == b || a == c || a == m || a == n || a == q ||
                                            b == c || b == m || b == n || b == q ||
                                            c == m || c == n || c == q ||
                                            m == n || m == q ||
                                            n == q};
                                        if (W)
                                            continue;
                                        if (x != w + w)
                                            continue;
                                        if (p != w + w + w)
                                            continue;
                                        cout << w << " " << x << " " << p << endl;
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
