#include <iostream>
using namespace std;

int main()
{
    int A[4][4] = {};
    for (int m{0}; m < 4; m++) 
    {
        for (int i1{0}; i1 < 4; i1++) 
        {
            for (int j1{i1 + 1}; j1 < 4; j1++) 
            {
                for (int i2{0}; i2 < 4; i2++) 
                {
                    for (int j2{i2 + 1}; j2 < 4; j2++) 
                    {
                        for (int i3{0}; i3 < 4; i3++) 
                        {
                            for (int j3{i3 + 1}; j3 < 4; j3++) 
                            {
                                int s{0}; 
                                for(int r=0; r<4; r++) {
                                    for(int c=0; c<4; ++c) {
                                        A[r][c] = 0;
                                    }
                                }
                                A[3][m] = 1;
                                A[0][i1] = 1;
                                A[0][j1] = 1; 
                                A[1][i2] = 1; 
                                A[1][j2] = 1; 
                                A[2][i3] = 1; 
                                A[2][j3] = 1; 
                                if (A[0][3] != 1 || A[3][3] != 0 || A[1][1] != 0) {
                                    continue; 
                                }
                                for (int a{0}; a < 4; a++) 
                                {
                                    int sum{0};
                                    for (int b{0}; b < 4; b++) 
                                        if (A[b][a] == 1) sum++;
                                    
                                    if (sum == 3) s++; 
                                }                                
                                int p{0}, q{0}; 
                                for (int a{0}; a < 4; a++) 
                                {
                                    if (A[0][a] == 1 && A[2][a] == 1) p++; 
                                    if (A[2][a] == 1 && A[3][a] == 1) p++; 
                                    if (A[1][a] == 1 && A[2][a] == 1) q++;
                                }                               
                                if (p > 0 || q == 0) continue; 
                                s++; 
                                int r{0}; 
                                for (int a{0}; a < 4; a++) 
                                {
                                    if (A[a][2] == 1 && A[a][3] == 1) r++;
                                }
                                if (r == 0) s++;                                
                                if (s == 3)
                                {
                                    for (int i{0}; i < 4; i++) 
                                    {
                                        int c{0}; 
                                        for (int j{0}; j < 4; j++) 
                                        {
                                            if (A[i][j] == 1)
                                            {
                                                if (c == 0)
                                                {
                                                    cout << j + 1;
                                                    c = 1;
                                                }
                                                else
                                                {
                                                    cout << " " << j + 1; 
                                                }
                                            }
                                        }
                                        cout << endl; 
                                    }
                                    return 0; 
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
