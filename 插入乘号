#include <iostream>
#include <string>
#include <cstdlib>
#include <vector>
#include <algorithm>
using namespace std;
vector<string>A;
vector<long long> D;
string s;
int k{ 0 };
void wx(int t,int h,int L) {
    if (t > k+1)return;
    if (t == k+1) {
        string ss1 = "";
        for (int i{ h };i < L;i++) {
            ss1 += s[i];
        }
        A.push_back(ss1);
        int LA = A.size();
        long long m{ 1 };
        for (int i{ 0 };i < LA;i++) {
            m *= stoll(A[i]);
        }
        D.push_back(m);
        A.pop_back();
        return;
    }
    for (int b{ h + 1 }; b< L-(k-t);b++) {
        string ss = "";
        for (int i{ h };i < b;i++)ss += s[i];
        A.push_back(ss);
        wx(t+1, b, L);
        A.pop_back();
    }
}
int main()
{
    cin >> s;
    int L = s.size();
    cin >> k;
    wx(1, 0, L);
    sort(D.begin(), D.end());
    int P = D.size();
    cout << D[P - 1];

}
