#include <iostream>
#include <string>
#include <vector>
#include <stack>
#include <map>

using namespace std;

// 定义运算符优先级
map<char, int> pr = {{'+', 1}, {'-', 1}, {'*', 2}, {'/', 2}};

// 辅助计算函数
void calc(stack<int>& nums, stack<char>& ops) {
    int b = nums.top(); nums.pop();
    int a = nums.top(); nums.pop();
    char op = ops.top(); ops.pop();
    if (op == '+') nums.push(a + b);
    else if (op == '-') nums.push(a - b);
    else if (op == '*') nums.push(a * b);
    else if (op == '/') nums.push(a / b);
}

void solve(string s) {
    stack<int> nums;
    stack<char> ops;
    // 预处理：防止开头就是负号或正号
    string t = "";
    for (int i = 0; i < s.length(); i++) {
        if (s[i] == ' ') continue;
        // 如果 '-' 或 '+' 在开头或紧跟 '('，补个 0
        if (s[i] == '-' || s[i] == '+') {
            if (i == 0 || s[i - 1] == '(') t += "0";
        }
        t += s[i];
    }

    for (int i = 0; i < t.length(); i++) {
        if (isdigit(t[i])) {
            int x = 0, j = i;
            while (j < t.length() && isdigit(t[j])) {
                x = x * 10 + (t[j++] - '0');
            }
            nums.push(x);
            i = j - 1;
        } else if (t[i] == '(') {
            ops.push('(');
        } else if (t[i] == ')') {
            while (ops.top() != '(') calc(nums, ops);
            ops.pop(); // 弹出 '('
        } else {
            // 当前运算符优先级小于等于栈顶，先算栈顶
            while (!ops.empty() && ops.top() != '(' && pr[ops.top()] >= pr[t[i]]) {
                calc(nums, ops);
            }
            ops.push(t[i]);
        }
    }
    while (!ops.empty()) calc(nums, ops);
    cout << nums.top() << endl;
}

int main() {
    string s;
    while (getline(cin, s)) {
        if (s.empty()) continue;
        solve(s);
    }
    return 0;
}
